<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <stroke
                android:width="2dp"
                android:color="#dbdbdc" />

            <solid android:color="#FFFFFF" />

            <corners
                android:bottomLeftRadius="@dimen/shape_edittext_corner_radius"
                android:bottomRightRadius="@dimen/shape_edittext_corner_radius"
                android:topLeftRadius="@dimen/shape_edittext_corner_radius"
                android:topRightRadius="@dimen/shape_edittext_corner_radius" />
        </shape>
    </item>

</layer-list>